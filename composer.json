{
    "name": "endroid/twitter",
    "description": "Endroid Twitter",
    "keywords": ["endroid", "twitter", "bundle", "symfony"],
    "homepage": "https://github.com/endroid/Twitter",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.4",
        "kriswallsmith/buzz": "^0.10"
    },
    "require-dev": {
        "symfony/http-foundation": "^2.3|^3.0",
        "phpunit/phpunit": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Twitter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Twitter\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
