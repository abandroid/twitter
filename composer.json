{
    "name": "endroid/twitter",
    "description": "Endroid Twitter",
    "keywords": ["endroid", "twitter", "bundle", "symfony", "php"],
    "homepage": "https://github.com/endroid/twitter",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": ">=7.1",
        "abraham/twitteroauth": "^0.9",
        "endroid/installer": "^1.0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0",
        "symfony/http-foundation": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Twitter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Twitter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
